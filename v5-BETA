    Tab17:AddTextbox({
    Name = "Add Warning Blacklist",
    Default = "User Here",
    TextDisappear = true,
    Callback = function(urBlacklisted)
        getgenv().PlrBlacklistedTable = getgenv().PlrBlacklistedTable or {}

        local globallyBlacklist = findplr(urBlacklisted)
        
