                            animTrack.Stopped:Wait()
                            copiedAnim:Stop()
                            copiedAnim:Destroy()
                        end)
                    end
                end
            end
            
            getgenv().copyAllWhitelistedAnims = true
            while getgenv().copyAllWhitelistedAnims == true do
            wait()
                copyAnimations()
            end
