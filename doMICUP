Tab1:AddToggle({
Name = "Rainbow Baseplate (NOT! FE)",
Default = false,
Callback = function(getRainbowBaseplate)
if getRainbowBaseplate then
    local ws = cloneref(game:GetService("Workspace")) or game:GetService("Workspace")
    local getFolder = ws:FindFirstChild("PartStorage") or ws:WaitForChild("PartStorage")
    local getpart = getFolder:FindFirstChildOfClass("Part") or getFolder:FindFirstChildWhichIsA("Part")
    repeat wait() until getFolder and getpart
if getpart and getFolder then
    getpart.Transparency = 0
    print(tostring(getpart))
    wait(.1)
    print("Setting Loop: "..tostring(getgenv().StartRainbowPart))
    getgenv().StartRainbowPart = true
    repeat wait() until getgenv().StartRainbowPart == true
    print("Set Loop: "..tostring(getgenv().StartRainbowPart))
    while getgenv().StartRainbowPart == true do
    wait()
        getpart.BrickColor = BrickColor.new("White")
        wait()
        print("BrickColor Updated: "..tostring(getpart.BrickColor))
        wait()
        getpart.BrickColor = BrickColor.new("Bright red")
        wait()
        print("BrickColor Updated: "..tostring(getpart.BrickColor))
        wait()
        getpart.BrickColor = BrickColor.new("Bright blue")
        wait()
        print("BrickColor Updated: "..tostring(getpart.BrickColor))
        wait()
        getpart.BrickColor = BrickColor.new("Hot pink")
        wait()
        print("BrickColor Updated: "..tostring(getpart.BrickColor))
        wait()
        getpart.BrickColor = BrickColor.new("New Yeller")
        wait()
        print("BrickColor Updated: "..tostring(getpart.BrickColor))
        wait()
        getpart.BrickColor = BrickColor.new("Teal")
        wait()
        print("BrickColor Updated: "..tostring(getpart.BrickColor))
        wait()
        getpart.BrickColor = BrickColor.new("Camo")
        wait()
        print("BrickColor Updated: "..tostring(getpart.BrickColor))
        wait()
        getpart.BrickColor = BrickColor.new("Royal purple")
        wait()
        print("BrickColor Updated: "..tostring(getpart.BrickColor))
        wait()
        getpart.BrickColor = BrickColor.new("Magenta")
        wait()
        print("BrickColor Updated: "..tostring(getpart.BrickColor))
        wait()
        getpart.BrickColor = BrickColor.new("Pink")
        wait()
        print("BrickColor Updated: "..tostring(getpart.BrickColor))
        wait()
        getpart.BrickColor = BrickColor.new("CGA brown")
        wait()
        print("BrickColor Updated: "..tostring(getpart.BrickColor))
    end
    else
        getgenv().StartRainbowPart = false
        return warn("nil")
    end
else
    print("Changing Loop: "..tostring(getgenv().StartRainbowPart))
    wait(.2)
    getgenv().StartRainbowPart = false
    wait()
    local ws = cloneref(game:GetService("Workspace")) or game:GetService("Workspace")
    local getFolder = ws:FindFirstChild("PartStorage") or ws:WaitForChild("PartStorage")
    local getpart = getFolder:FindFirstChildOfClass("Part") or getFolder:FindFirstChildWhichIsA("Part")
    repeat wait() until getFolder and getpart
    getpart.Transparency = 1
    wait(.1)
    getpart.BrickColor = BrickColor.new("Medium stone grey")
    wait()
    repeat wait() until getgenv().StartRainbowPart == false and getpart.Transparency == 1
    wait()
    print("Toggle Loop Set To: "..tostring(getgenv().StartRainbowPart))
end
end})

Tab11:AddTextbox({
Name = "18+ Booth Image",
Default = "Text For Booth",
TextDisappear = true,
Callback = function(TextHereForBooth)
local function getStall()
    for i,v in pairs(game:GetService("Workspace").Stalls:GetChildren()) do
        if v.Player.Value == game:GetService("Players").LocalPlayer then
            return v
        end
    end
return nil
end

local LocalStall = getStall()
if LocalStall ~= nil and LocalStall:FindFirstChild("Edit") then
local TheStall = LocalStall
local args = {
    [1] = TextHereForBooth,
    [2] = "5888213893"
}

TheStall:FindFirstChild("Edit"):FireServer(unpack(args))
else
local function getStall()
        for i,v in pairs(game:GetService("Workspace").Stalls:GetChildren()) do
            if v.Player.Value == game:GetService("Players").LocalPlayer then
                return v
            end
        end
    return nil
end
    for i,v in pairs(game:GetService("Workspace").Stalls:GetChildren()) do
        if v and v ~= nil then
            game:GetService("Players").LocalPlayer.Character:PivotTo(v:GetPivot())
            wait(0.2)
            fireproximityprompt(v:FindFirstChild("ProxPart").ProximityPrompt)
            wait(0.2)
            repeat wait() until v.Player.Value == game:GetService("Players").LocalPlayer
            wait()
            if v.Player.Value == game:GetService("Players").LocalPlayer then
                local ProximityPrompt = v:FindFirstChild("ProxPart"):FindFirstChild("ProximityPrompt")
                wait(0.2)
                fireproximityprompt(ProximityPrompt, 10)
                wait(1)
                local args = {
                    [1] = tostring(TextHereForBooth),
                    [2] = "5888213893"
                }
            
                getStall()["Edit"]:FireServer(unpack(args))
                wait()
                local Signals = {"Activated", "MouseButton1Down", "MouseButton2Down", "MouseButton1Click", "MouseButton2Click"}
                local Button = game:GetService("Players").LocalPlayer.PlayerGui.StallLocal.StallFrame
            
                for _,v in pairs(Button:GetDescendants()) do
                    if v:IsA("ImageButton") or v:IsA("TextButton") and v.Name == "CancelButton" then
                        for i,Signal in pairs(Signals) do
                            firesignal(v[Signal])
                        end
                    end
                end
            end
        end
    end
end
end})

Tab7:AddTextbox({
Name = "Whitelist User",
Default = "User Here",
TextDisappear = true,
Callback = function(enterUser)
    local getTheirUser = findplr(enterUser)

    if not getTheirUser then
        return OrionLib:MakeNotification({
            Name = "Error: Not Found",
            Content = "Player was not found!",
            Image = "rbxassetid://4483345998",
            Time = 10
        })
    end

    if table.find(whitelist, getTheirUser.Name) then
        return OrionLib:MakeNotification({
            Name = "Error:",
            Content = "Player is already on the whitelist.",
            Image = "rbxassetid://4483345998",
            Time = 10
        })
    end

    addToWhitelist(getTheirUser.Name)
    return OrionLib:MakeNotification({
        Name = "Success!",
        Content = tostring(getTheirUser.Name).." was added to the whitelist.",
        Image = "rbxassetid://4483345998",
        Time = 10
    })    
end})

Tab7:AddTextbox({
Name = "Remove Whitelist User",
Default = "User Here",
TextDisappear = true,
Callback = function(enterUser)
    local getTheirUser = findplr(enterUser)

    if not getTheirUser then
        return OrionLib:MakeNotification({
            Name = "Error: Not Found",
            Content = "Player was not found!",
            Image = "rbxassetid://4483345998",
            Time = 10
        })
    end

    local playerName = getTheirUser.Name:lower()

    if not table.find(whitelist, playerName) then
        return OrionLib:MakeNotification({
            Name = "Error:",
            Content = "Player is not in the whitelist.",
            Image = "rbxassetid://4483345998",
            Time = 10
        })
    end

    removeFromWhitelist(playerName)
    return OrionLib:MakeNotification({
        Name = "Success!",
        Content = tostring(getTheirUser.Name).." was removed from the whitelist.",
        Image = "rbxassetid://4483345998",
        Time = 10
    })    
end})

Tab11:AddTextbox({
Name = "UnClaim A Booth",
Default = "UserName or Display",
TextDisappear = true,
Callback = function(LolUnClaim)
    local Players = cloneref(game:GetService("Players")) or game:GetService("Players")
    local Workspace = game:GetService("Workspace")
    local LocalPlayer = Players.LocalPlayer
    local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local HumanoidRootPart = Character:FindFirstChild("HumanoidRootPart") or Character:WaitForChild("HumanoidRootPart")
    local stalls = Workspace:WaitForChild("Stalls")
    
    local OldestCF = Character:WaitForChild("HumanoidRootPart").CFrame
    local LolBigDawg = findplr(LolUnClaim)

    local function getStallFunc()
        local LolBigDawg = findplr(LolUnClaim)
        for _, stall in pairs(stalls:GetChildren()) do
            if stall:FindFirstChild("ProxPart") and stall:FindFirstChild("Player") then
                if stall.Player.Value ~= "M1RD3RCAUGHT" and stall.Player.Value ~= "STK_kets4eki" then
                    return stall
                else
                    return OrionLib:MakeNotification({
                        Name = "Error: Owner",
                        Content = "Owner of script owns this booth!",
                        Image = "rbxassetid://4483345998",
                        Time = 10
                    })
                end
            end
        end
        return nil
    end

    local function getStallSelf()
        for _, v in pairs(Stalls:GetChildren()) do
            if v.Player.Value == game.Players.LocalPlayer then
                return v
            end
        end
        return nil
    end
    
    wait()
    
    local stall = getStallFunc()
    
    if stall and stall ~= nil and fireproximityprompt then
        local Players = cloneref(game:GetService("Players")) or game:GetService("Players")
        local LolBigDawg = findplr(LolUnClaim)

        if not LolBigDawg then
            return OrionLib:MakeNotification({
                Name = "Error: Not Found",
                Content = "Player was not found!",
                Image = "rbxassetid://4483345998",
                Time = 10
            })
        end

        if not Players[LolBigDawg.Name] then
            return OrionLib:MakeNotification({
                Name = "Error: Not Found",
                Content = "Player was not found!",
                Image = "rbxassetid://4483345998",
                Time = 10
            })
        end

        local function makeInvisible(character)
            if character then
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") or part:IsA("Decal") or part:IsA("SurfaceGui") then
                        part.Transparency = 1
                    end
                end
            end
        end

        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character then
                makeInvisible(player.Character)
            end
        end

        local Proximity1 = stall:FindFirstChild("ProxPart").ProximityPrompt
        LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = CFrame.new(stall:FindFirstChild("ProxPart").Position)
        wait(0.3)
        fireproximityprompt(Proximity1, 10)

        wait(0.5)
        getStallSelf():WaitForChild("CloseStall", 1):FireServer()

        local Button = LocalPlayer.PlayerGui.StallLocal.StallFrame
        local Signals = {"Activated", "MouseButton1Down", "MouseButton2Down", "MouseButton1Click", "MouseButton2Click"}
        
        for _, v in pairs(Button:GetDescendants()) do
            if (v:IsA("ImageButton") or v:IsA("TextButton")) and v.Name == "CancelButton" then
                for _, Signal in pairs(Signals) do
                    firesignal(v[Signal])
                end
            end
        end
        wait(0.5)
        local function makeCharacterVisible(character)
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.Transparency = 0
                end
            end
        end
        
        for _, player in pairs(game.Players:GetPlayers()) do
            if player ~= game.Players.LocalPlayer and player.Character then
                makeCharacterVisible(player.Character)
            end
        end
        wait()
        local function makePartInvisible(part)
            part.Transparency = 1
        end
        
        local function makeHumanoidRootPartInvisible(character)
            local player = game.Players:GetPlayerFromCharacter(character)
            if player and player ~= game.Players.LocalPlayer then
                local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
                if humanoidRootPart then
                    makePartInvisible(humanoidRootPart)
                end
            end
        end
        
        for _, player in pairs(game.Players:GetPlayers()) do
            if player.Character then
                makeHumanoidRootPartInvisible(player.Character)
            end
        end
    end    
end})

