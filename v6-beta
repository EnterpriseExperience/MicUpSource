    function do_baseplate_check()
        local screenGui = Instance.new("ScreenGui") 
        screenGui.IgnoreGuiInset = true
        screenGui.Name = "BASEPLATE_LOADING"
        screenGui.Parent = getgenv().LocalPlayer:WaitForChild("PlayerGui")
        
