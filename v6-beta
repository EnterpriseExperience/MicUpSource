            getgenv().FreezingChar = true
            if Character and HumanoidRootPart or Character:FindFirstChild("HuamnoidRootPart") then
                if getgenv().FreezingChar == true then
                    HumanoidRootPart.Anchored = true
                end
            else
                getgenv().FreezingChar = false
            end
        else
