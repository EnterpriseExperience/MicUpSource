                    Duration = 7,
                    Image = 4483362458,
                    Actions = {
                       Ignore = {
                        Name = "Okay!",
                        Callback = function()
                            print("...")
                        end
                       },
                    },
                })
            end
        
            local colorsList = {
                "Teal",
                "Gray",
                "Red",
            }
        
            local function pickRandomColor()
                local randomIndex = math.random(1, #colorsList)
                return colorsList[randomIndex]
            end
        
